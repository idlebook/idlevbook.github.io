<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>+ (NSString *)googleReverseStringWithCoordinate:(CLLocationCoordinate2D)coordinate {
    return [NSString stringWithFormat:@"http://maps.google.com/maps/geo?q=%lf,%lf&amp;output=json&amp;sensor=false&amp;accuracy=4", coordinate.latitude,coordinate.longitude];
}
