         <span class="n">_572_å¦ä¸€ä¸ªæ ‘çš„å­æ ‘</span> <span class="n">o</span> <span class="o">=</span> <span class="k">new</span> <span class="n">_572_å¦ä¸€ä¸ªæ ‘çš„å­æ ‘</span><span class="o">();</span>
         <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">o</span><span class="o">.</span><span class="na">preSerialize</span><span class="o">(</span><span class="n">root</span><span class="o">));</span>
         <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">o</span><span class="o">.</span><span class="na">preSerialize</span><span class="o">(</span><span class="n">root2</span><span class="o">));</span>
